<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.app.pushup">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="Re:PushUp"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.RePushup">
        <activity android:name=".FinishPushupActivity"></activity>
        <activity android:name=".RestActivity" />
        <activity android:name=".PushupActivity" />
        <activity android:name=".SettingsActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>